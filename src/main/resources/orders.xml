<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="hashMapOrderRepository" class="pl.training.shop.orders.HashMapOrderRepository"/>

    <bean id="orderService" class="pl.training.shop.orders.OrderService">
        <constructor-arg name="orderRepository" ref="hashMapOrderRepository"/>
    </bean>

</beans>